{
	"name": "cell-count-integrate",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"db:dev:rm": "docker compose rm dev-db -s -f -v",
		"db:dev:up": "docker compose up dev-db -d",
		"db:dev:restart": "npm run db:dev:rm && npm run db:dev:up",
		"db:test:rm": "docker compose rm test-db -s -f -v",
		"db:test:up": "docker compose up test-db -d",
		"db:test:restart": "npm run db:test:rm && npm run db:test:up",
		"prebuild": "rimraf dist",
		"build": "npm run prebuild && nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e": "npm run db:test:restart",
		"test:e2e": "dotenv -e .env.test -- jest --watchAll --no-cache --config ./test/jest-e2e.json"
	},
	"engines": {
		"node": ">= 20.18.0"
	},
	"dependencies": {
		"@nestjs/common": "^10.4.4",
		"@nestjs/core": "^10.4.4",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/mapped-types": "^2.0.5",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.0.0",
		"@nestjs/swagger": "^7.4.2",
		"@nestjs/typeorm": "^10.0.2",
		"argon2": "^0.41.1",
		"aws-sdk": "^2.1691.0",
		"axios": "^1.7.7",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"dotenv": "^16.4.5",
		"dotenv-expand": "^11.0.6",
		"lodash": "^4.17.21",
		"mysql2": "^3.11.3",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"typeorm": "^0.3.20",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.2",
		"@types/lodash": "^4.17.10",
		"@types/multer": "^1.4.12",
		"@types/node": "^20.3.1",
		"@types/passport-jwt": "^4.0.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.5.0",
		"pactum": "^3.7.1",
		"prettier": "^3.0.0",
		"rimraf": "^6.0.1",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.6.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
